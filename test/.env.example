# LangChain End-to-End Test Environment Configuration Example
# Copy this file to .env and fill in actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# LLM Provider Configuration
TEST_LLM_PROVIDER=openai
TEST_LLM_MODEL=gpt-3.5-turbo

# Simulated Node-RED Environment
TEST_CONFIG_NODE_ID=test-config-node
TEST_FLOW_ID=test-flow-123
TEST_NODE_ID=test-node-456

# Test Web Server Configuration
TEST_WEB_PORT=3001

# Whether to enable real LLM calls (will call real API when set to true)
ENABLE_REAL_LLM_CALLS=false

# Test Configuration
TEST_SESSION_TIMEOUT=30000
TEST_API_DELAY=1000

# Debug Mode
DEBUG_MODE=true
LOG_LEVEL=info